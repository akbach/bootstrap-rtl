.bs-docs-section.clearfix
    .row
        .col-lg-12
            .page-header
                h1#navbar #{t['navbar']}
            .bs-component
                nav.navbar.navbar-default
                    .container-fluid
                        .navbar-header
                            button.navbar-toggle.collapsed(type="button", data-toggle="collapse", data-target="#bs-example-navbar-collapse-1")
                                span.sr-only #{t['Toggle navigation']}
                                span.icon-bar
                                span.icon-bar
                                span.icon-bar
                            a.navbar-brand(href="#") #{t['Brand']}
                        #bs-example-navbar-collapse-1.collapse.navbar-collapse
                            ul.nav.navbar-nav
                                li.active
                                    a(href="#")
                                        | #{t['Link']}
                                        span.sr-only (#{t['current']})
                                li
                                    a(href="#") #{t['Link']}
                                li.dropdown
                                    a.dropdown-toggle(href="#", data-toggle="dropdown", role="button", aria-expanded="false")
                                        | #{t['Dropdown']}&nbsp;
                                        span.caret
                                    ul.dropdown-menu(role="menu")
                                        li
                                            a(href="#") #{t['Action']}
                                        li
                                            a(href="#") #{t['Another action']}
                                        li
                                            a(href="#") #{t['Something else here']}
                                        li.divider
                                        li
                                            a(href="#") #{t['Separated link']}
                                        li.divider
                                        li
                                            a(href="#") #{t['One more separated link']}
                            form.navbar-form.navbar-left(role="search")
                                .form-group
                                    input.form-control(type="text", placeholder="#{t['Search']}")
                                button.btn.btn-default(type="submit") #{t['Submit']}
                            ul.nav.navbar-nav.navbar-right
                                li
                                    a(href="#") #{t['Link']}
